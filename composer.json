{
    "name": "kynkai/explorer-php",
    "type": "library",
    "description": "A file manager application that is included with releases",
    "license": "MIT",
    "keywords": [
        "file manager",
        "manager",
        "cache"
    ],
    "authors": [
        {
            "name": "QuachVanHao",
            "email": "kynkai2020@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Explorer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": ["src/functions.php"]
    },
    "require": {
        "league/omnipay": "^3.0",
        "omnipay/authorizenet": "3.x@dev"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
}
